<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.re.mind.dao.userDao">
	<insert id="joinuser" parameterType="string" >
		insert into user values(#{id},#{pwd},#{name},1)
	</insert>
	<select id="overlapcheck" parameterType="string" resultType="int">
		select count(*) from user where id=#{id}
	</select>
	<select id="pwdcheck" parameterType="string" resultType="string">
		select pwd from user where id=#{id}
	</select>
	
	
</mapper>